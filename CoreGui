-- List of banned hardware IDs
local bannedHardwareIds = {
    "nil",
    -- Add more banned hardware IDs here
}

-- Function to check if a hardware ID is banned
local function isHardwareIdBanned(hardwareId)
    for _, bannedHardwareId in ipairs(bannedHardwareIds) do
        if bannedHardwareId == hardwareId then
            return true
        end
    end
    return false
end

-- Example usage: hardwareId to check
-- In a real scenario, you would dynamically get this hardwareId
local hardwareId = "nil" -- Replace with the actual hardware ID you want to check

-- Check if the hardware ID is banned
if isHardwareIdBanned then
    game.Players.LocalPlayer:Kick("You Cant Use This Script , Ban SecurityDetect 2.0 - Your banned You ur not able to use this script ")
    return  -- Stop further execution of the script
end

-- Continue with the rest of the script if not banned

wait(4)

